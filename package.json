{
  "name": "playing-poker",
  "version": "1.0.0",
  "description": "NodeJS app to manage project tasks",
  "repository": "https://github.com/velmyk/playing-poker.git",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf target/build/*",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "npm-run-all --parallel client server",
    "client": "webpack && webpack-dev-server --devtool source-map",
    "server": "nodemon index.js --watch ./server",
    "build": "webpack -p --optimize-compress-warnings=false",
    "test": "karma start",
    "tdd": "karma start --singleRun=false",
    "css": "node-sass --output-style compressed ./src/scss/core.scss ./target/build/app.css",
    "static": "cp ./src/index.html ./target/build/index.html",
    "autoprefixer": "postcss -u autoprefixer -r target/build/*.css",
    "lint:ui": "eslint src/",
    "lint:server": "eslint server/",
    "lint": "npm-run-all lint:*",
    "uglify": "uglifyjs target/build/app.js -m -c -o target/build/app.js",
    "app": "npm-run-all clean build css static uglify"
  },
  "keywords": [
    "NodeJS",
    "Socket.io",
    "Express",
    "MongoDB",
    "Playing poker",
    "PassportJS"
  ],
  "author": "velmyk",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "1.5.0",
    "autoprefixer": "6.3.4",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "base64-font-loader": "0.0.3",
    "chai": "3.5.0",
    "chalk": "1.1.1",
    "core-js": "2.0.3",
    "coveralls": "2.11.8",
    "eslint": "2.4.0",
    "foreman": "1.4.1",
    "js-beautify": "1.6.2",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon-chai": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "ng-annotate-loader": "0.1.0",
    "node-dir": "0.1.11",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "npm-run-all": "1.5.1",
    "phantomjs": "1.9.8",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "2.1.4",
    "postcss-cli": "2.5.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "uglify-js": "2.6.2",
    "watch": "0.17.1",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1",
    "webpack-polyfills-plugin": "0.0.9",
    "webpack-spritesmith": "0.0.3"
  },
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.2",
    "angular-aria": "1.5.2",
    "angular-cookies": "1.5.2",
    "angular-material": "1.0.6",
    "angular-messages": "1.5.2",
    "angular-ui-router": "0.2.18",
    "body-parser": "~1.5.0",
    "composable-middleware": "^0.3.0",
    "cookie-parser": "~1.0.1",
    "cors": "2.7.1",
    "crypto": "0.0.3",
    "dotenv": "2.0.0",
    "errorhandler": "~1.0.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "5.7.0",
    "lodash": "~2.4.1",
    "method-override": "~1.0.0",
    "mongoose": "^4.4.6",
    "morgan": "~1.0.0",
    "ng-file-upload": "12.0.4",
    "passport": "^0.3.0",
    "passport-github": "1.1.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "serve-favicon": "2.3.0",
    "socket.io": "1.4.5",
    "socket.io-client": "1.4.5"
  },
  "engines": {
    "node": "4.3.1"
  }
}
