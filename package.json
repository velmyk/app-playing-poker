{
  "name": "playing-poker",
  "version": "1.0.0",
  "description": "NodeJS app to manage project tasks",
  "repository": "https://github.com/velmyk/playing-poker.git",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf target/*",
    "starttt": "nf start",
    "serve": "node node_modules/.bin/webpack-dev-server --content-base app",
    "dev": "nodemon index.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "npm-run-all --parallel client server",
    "client": "webpack && webpack-dev-server --devtool source-map",
    "server": "nodemon ./server/index.js --watch ./server",
    "prebuild": "npm i",
    "build": "webpack -p --optimize-compress-warnings=false",
    "posttestt": "coverage-blamer -c ./coverage/coverage-final.json -s . > ./coverage-blamer.md",
    "test": "karma start",
    "tdd": "karma start --singleRun=false"
  },
  "keywords": [
    "NodeJS",
    "Socket.io",
    "Express",
    "MongoDB",
    "Playing poker",
    "PassportJS"
  ],
  "author": "velmyk",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "1.5.0",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "base64-font-loader": "0.0.3",
    "chai": "3.5.0",
    "chalk": "1.1.1",
    "copy-webpack-plugin": "0.3.3",
    "core-js": "2.0.3",
    "cors": "2.7.1",
    "coverage-blamer": "0.1.8",
    "coveralls": "2.11.8",
    "css-loader": "0.23.1",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.5",
    "foreman": "1.4.1",
    "html-webpack-plugin": "1.7.0",
    "import-glob-loader": "1.1.0",
    "isparta": "3.0.4",
    "isparta-loader": "0.2.0",
    "js-beautify": "1.6.2",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "1.0.0",
